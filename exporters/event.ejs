<%- begin %> <%- typeAndName %><%
if (params.type == 'RECURRING') { %>
	ON SCHEDULE EVERY <%- params.intervalValue %> <%- params.intervalField %><%
	if (params.starts) { %>
		STARTS <%- params.starts %><%
	}
	if (params.ends) { %>
		ENDS <%- params.ends %><%
	}
} else { %>
	ON SCHEDULE AT <%- params.executeAt %><%
} %>
	ON COMPLETION <%- params.onCompletion %>
	<%- params.status %><%
if (params.comment) { %>
	COMMENT <%- params.comment %><%
} %>
DO <%- params.body %><%- end %>
